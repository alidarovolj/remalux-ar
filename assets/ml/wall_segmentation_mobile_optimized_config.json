{
  "model_info": {
    "name": "wall_segmentation_mobile_ultra_fast",
    "version": "3.0",
    "description": "Ультра-быстрая модель для мобильных устройств с целевым временем <30ms",
    "input_size": [128, 128],
    "output_classes": 2,
    "architecture": "MobileNetV3-Small + DeepLabV3Lite",
    "target_fps": 60,
    "target_latency_ms": 25
  },
  "performance": {
    "use_xnnpack": true,
    "use_gpu_delegate": true,
    "preferred_threads": 2,
    "optimization_level": "ULTRA_FAST",
    "memory_optimization": true,
    "quantization": "INT8"
  },
  "processing": {
    "preprocessing": {
      "normalize": false,
      "resize_method": "nearest",
      "input_range": [0, 1]
    },
    "postprocessing": {
      "apply_morphology": false,
      "confidence_threshold": 0.3,
      "min_area_threshold": 50,
      "max_area_threshold": 0.9,
      "use_connected_components": false
    }
  },
  "quality_thresholds": {
    "min_wall_percentage": 0.01,
    "max_wall_percentage": 0.99,
    "confidence_threshold": 0.2
  },
  "optimization_flags": {
    "skip_similar_frames": true,
    "cache_results": true,
    "use_fast_resize": true,
    "aggressive_memory_management": true
  }
}
