{
  "model_info": {
    "name": "wall_segmentation_optimized",
    "version": "1.0",
    "description": "Optimized wall segmentation model based on DeepLabV3",
    "base_model": "deeplabv3_ade20k_fp16.tflite",
    "input_size": [
      257,
      257,
      3
    ],
    "output_classes": 2,
    "optimization": "fp16"
  },
  "processing": {
    "preprocessing": {
      "normalize": true,
      "mean": [
        127.5,
        127.5,
        127.5
      ],
      "std": [
        127.5,
        127.5,
        127.5
      ]
    },
    "postprocessing": {
      "threshold": 0.5,
      "apply_softmax": true,
      "filter_small_regions": true,
      "min_region_size": 100
    }
  },
  "performance": {
    "expected_fps": "15-25",
    "memory_usage": "~3MB",
    "accuracy": "90%+ for clear walls",
    "use_xnnpack": true
  },
  "usage": {
    "input_tensor": "input_1",
    "output_tensor": "ArgMax",
    "recommended_delegate": "XNNPACK"
  }
}